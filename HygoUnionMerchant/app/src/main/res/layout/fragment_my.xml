<?xml version="1.0" encoding="utf-8"?>
<com.zyd.wlwsdk.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                            xmlns:app="http://schemas.android.com/apk/res-auto"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:background="@color/bg_color"
                                            android:orientation="vertical">

    <com.zyd.wlwsdk.autolayout.AutoRelativeLayout
        android:id="@+id/rl_title"
        android:layout_width="match_parent"
        android:layout_height="88px"
        android:background="@color/white">

        <TextView
            android:id="@+id/tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="我的"
            android:textColor="@color/black_333"
            android:textSize="36px"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_alignParentBottom="true"
            android:background="@color/gray_ccc"/>
    </com.zyd.wlwsdk.autolayout.AutoRelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:paddingBottom="30px">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="200px"
                android:background="@color/white">

                <ImageView
                    android:id="@+id/iv_image"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="30px"/>

                <TextView
                    android:id="@+id/tv_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="25px"
                    android:layout_marginTop="55px"
                    android:layout_toRightOf="@id/iv_image"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text=""
                    android:textColor="@color/black_333"
                    android:textSize="32px"/>

                <TextView
                    android:id="@+id/tv_yirenzheng"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20px"
                    android:layout_marginTop="62px"
                    android:layout_toRightOf="@id/tv_name"
                    android:drawableLeft="@drawable/ic_yirenzheng"
                    android:drawablePadding="10px"
                    android:gravity="center"
                    android:text="已认证"
                    android:textColor="@color/orange_debf64"
                    android:textSize="24px"
                    android:visibility="gone"/>

                <TextView
                    android:id="@+id/tv_phone_num"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tv_name"
                    android:layout_marginLeft="25px"
                    android:layout_marginRight="30px"
                    android:layout_marginTop="20px"
                    android:layout_toRightOf="@id/iv_image"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:textColor="@color/gray_999"
                    android:textSize="28px"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_my_zizhirenzheng"
                android:layout_width="match_parent"
                android:layout_height="96px"
                android:layout_marginTop="20px"
                android:background="@drawable/bg_btn_white"
                android:visibility="gone">

                <ImageView
                    android:layout_width="40px"
                    android:layout_height="40px"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="30px"
                    android:src="@drawable/ic_renzheng"
                    app:layout_auto_baseheight="width"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="93px"
                    android:text="认证后方可提取积分"
                    android:textColor="@color/black_333"
                    android:textSize="32px"/>

                <ImageView
                    android:id="@+id/iv_zizhirenzheng_arrow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="30px"
                    android:src="@drawable/ic_arrow_right"
                    android:visibility="gone"/>

                <TextView
                    android:id="@+id/tv_zizhirenzheng_state"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="65px"
                    android:text=""
                    android:textColor="@color/gray_ccc"
                    android:textSize="28px"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_my_account"
                android:layout_width="match_parent"
                android:layout_height="110px"
                android:layout_marginTop="20px"
                android:background="@drawable/bg_btn_white">

                <ImageView
                    android:id="@+id/iv_account_image"
                    android:layout_width="40px"
                    android:layout_height="40px"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="45px"
                    app:layout_auto_baseheight="width"/>

                <TextView
                    android:id="@+id/tv_account_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="137px"
                    android:text=""
                    android:textColor="@color/black_333"
                    android:textSize="32px"/>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="30px"
                    android:src="@drawable/ic_arrow_right"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_my_income"
                android:layout_width="match_parent"
                android:layout_height="110px"
                android:background="@drawable/bg_btn_white"
                android:visibility="gone">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:layout_marginLeft="137px"
                    android:background="@color/gray_e0e0e0"/>

                <ImageView
                    android:layout_width="40px"
                    android:layout_height="40px"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="45px"
                    android:src="@drawable/ic_wodshouyi"
                    app:layout_auto_baseheight="width"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="137px"
                    android:text="我的收益"
                    android:textColor="@color/black_333"
                    android:textSize="32px"/>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="30px"
                    android:src="@drawable/ic_arrow_right"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_printer"
                android:layout_width="match_parent"
                android:layout_height="110px"
                android:background="@drawable/bg_btn_white"
                android:visibility="gone">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:layout_marginLeft="137px"
                    android:background="@color/gray_e0e0e0"/>

                <ImageView
                    android:layout_width="40px"
                    android:layout_height="40px"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="45px"
                    android:src="@drawable/ic_wodshouyi"
                    app:layout_auto_baseheight="width"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="137px"
                    android:text="我的打印机"
                    android:textColor="@color/black_333"
                    android:textSize="32px"/>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="30px"
                    android:src="@drawable/ic_arrow_right"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_bank_card"
                android:layout_width="match_parent"
                android:layout_height="110px"
                android:layout_marginTop="20px"
                android:background="@drawable/bg_btn_white"
                android:visibility="gone">

                <ImageView
                    android:layout_width="40px"
                    android:layout_height="40px"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="45px"
                    android:src="@drawable/ic_wodyinghangka"
                    app:layout_auto_baseheight="width"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="137px"
                    android:text="我的银行卡"
                    android:textColor="@color/black_333"
                    android:textSize="32px"/>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="30px"
                    android:src="@drawable/ic_arrow_right"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_alipay"
                android:layout_width="match_parent"
                android:layout_height="110px"
                android:background="@drawable/bg_btn_white"
                android:visibility="gone">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:layout_marginLeft="137px"
                    android:background="@color/gray_e0e0e0"/>

                <ImageView
                    android:layout_width="40px"
                    android:layout_height="40px"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="45px"
                    android:src="@drawable/ic_wodzhifubao"
                    app:layout_auto_baseheight="width"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="137px"
                    android:text="我的支付宝"
                    android:textColor="@color/black_333"
                    android:textSize="32px"/>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="30px"
                    android:src="@drawable/ic_arrow_right"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_user_management"
                android:layout_width="match_parent"
                android:layout_height="110px"
                android:layout_marginTop="20px"
                android:background="@drawable/bg_btn_white"
                android:visibility="gone">

                <ImageView
                    android:layout_width="40px"
                    android:layout_height="40px"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="45px"
                    android:src="@drawable/ic_zhagnhuguanli"
                    app:layout_auto_baseheight="width"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="137px"
                    android:text="用户管理"
                    android:textColor="@color/black_333"
                    android:textSize="32px"/>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="30px"
                    android:src="@drawable/ic_arrow_right"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_account_management"
                android:layout_width="match_parent"
                android:layout_height="110px"
                android:background="@drawable/bg_btn_white">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:layout_marginLeft="137px"
                    android:background="@color/gray_e0e0e0"/>

                <ImageView
                    android:layout_width="40px"
                    android:layout_height="40px"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="45px"
                    android:src="@drawable/ic_zhagnhuguanli"
                    app:layout_auto_baseheight="width"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="137px"
                    android:text="账户管理"
                    android:textColor="@color/black_333"
                    android:textSize="32px"/>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="30px"
                    android:src="@drawable/ic_arrow_right"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_bingding_hygo"
                android:layout_width="match_parent"
                android:layout_height="110px"
                android:background="@drawable/bg_btn_white"
                android:visibility="gone">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:layout_marginLeft="137px"
                    android:background="@color/gray_e0e0e0"/>

                <ImageView
                    android:layout_width="40px"
                    android:layout_height="40px"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="45px"
                    android:src="@drawable/ic_bangdignhuanyougouzhanghu"
                    app:layout_auto_baseheight="width"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="137px"
                    android:text="绑定环游购账户"
                    android:textColor="@color/black_333"
                    android:textSize="32px"/>

                <ImageView
                    android:id="@+id/iv_binding_arrow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="30px"
                    android:src="@drawable/ic_arrow_right"/>

                <TextView
                    android:id="@+id/tv_bangding_state"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="65px"
                    android:text=""
                    android:textColor="@color/gray_ccc"
                    android:textSize="28px"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_setting"
                android:layout_width="match_parent"
                android:layout_height="110px"
                android:layout_marginTop="20px"
                android:background="@drawable/bg_btn_white">

                <ImageView
                    android:layout_width="40px"
                    android:layout_height="40px"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="45px"
                    android:src="@drawable/ic_shezhi"
                    app:layout_auto_baseheight="width"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="137px"
                    android:text="设置"
                    android:textColor="@color/black_333"
                    android:textSize="32px"/>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="30px"
                    android:src="@drawable/ic_arrow_right"/>

                <View
                    style="@style/line_horizontal"
                    android:layout_marginLeft="137px"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_suggestion"
                android:layout_width="match_parent"
                android:layout_height="110px"
                android:background="@drawable/bg_btn_white">

                <ImageView
                    android:layout_width="40px"
                    android:layout_height="40px"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="45px"
                    android:src="@drawable/ic_shezhi"
                    app:layout_auto_baseheight="width"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="137px"
                    android:text="意见反馈"
                    android:textColor="@color/black_333"
                    android:textSize="32px"/>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="30px"
                    android:src="@drawable/ic_arrow_right"/>

            </RelativeLayout>

        </LinearLayout>

    </ScrollView>

</com.zyd.wlwsdk.autolayout.AutoLinearLayout>